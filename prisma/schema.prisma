// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model SnorlaxMoves {
    id              Int     @id @default(autoincrement())
    identifier      String  @unique
    type            String
    name            String
    damage          Int
    pp              Int
    imageSrc        String?
}
